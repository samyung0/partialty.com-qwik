{
  "name": "partialty",
  "description": "Online E-Learning platform for coding",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "pre-commit": [
    "lint.fix",
    "fmt",
    "git:add"
  ],
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "compile.chokidar": "del-cli ./src/assets/chokidar/index.txt && cd compile && ncc build ./chokidarEntry.js -o ../src/assets/chokidar -m && cd .. && rname ./src/assets/chokidar/index.js ./src/assets/chokidar/index.txt",
    "db:dev": "drizzle-kit studio --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:check": "drizzle-kit check:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:drop": "drizzle-kit drop --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:migrate": "cd ./drizzle_turso && bun migrate.dev.ts",
    "db:dev:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:prod": "drizzle-kit studio --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:drop": "drizzle-kit drop --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:migrate": "cd ./drizzle_turso && bun migrate.prod.ts",
    "db:prod:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "deploy": "gcloud run deploy my-cloud-run-app --source .",
    "dev": "concurrently -c \"auto\" --handle-input \"vite --mode ssr\" \"bun --hot bun/index.ts\"",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev:email": "email dev",
    "dev:server": "bun --watch bun/index.ts",
    "fmt": "eslint \"src/**/*.ts*\" --fix && prettier --write .",
    "fmt.check": "prettier --check .",
    "git:add": "git add .",
    "lint": "eslint \"src/**/*.ts*\" && tsc --noEmit",
    "lint.fix": "eslint \"src/**/*.ts*\" --fix",
    "posttest:integration": "rm local.db",
    "pretest:integration": "bun run ./setup/migrate.test.ts",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US,zh-HK,ja-JP --assetsPath=i18n",
    "serve": "wrangler pages dev ./dist --compatibility-flags=nodejs_als",
    "start:server": "bun bun/index.ts",
    "test": "vitest run --config ./vitest.config.ts",
    "test:integration": "vitest run --config ./vitest.config.integration.ts",
    "test:ws": "cd bun && artillery run test.yaml -o test.json",
    "test:ws:demo": "cd bun && artillery run demo.yaml -o demo.json"
  },
